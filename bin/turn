#!/usr/bin/env ruby
require_relative '../lib/turn'
require_relative '../lib/display_board'
require_relative '../lib/move'
require_relative '../lib/valid_move'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"

move(board, 4, "X")
move(board, 0, "O")
move(board, 1, "X")

display_board(board)
turn(board)
display_board(board)
